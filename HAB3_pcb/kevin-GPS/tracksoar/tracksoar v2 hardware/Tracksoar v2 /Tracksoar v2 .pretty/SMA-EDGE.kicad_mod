(module SMA-EDGE (layer F.Cu) (tedit 0)
  (descr "<b>SMA Antenna Connector</b><p>\nThis is a footprint for an edge mount RF antenna. Works pretty well with SMA type connectors but may also work with other edge mount RF connectors. Keep in mind, these edge mount connectors assume you are using a 0.062\" PCB thickness.")
  (fp_text reference J$1 (at 0 0) (layer F.SilkS) hide
    (effects (font (size 1.27 1.27) (thickness 0.15)))
  )
  (fp_text value SMA_EDGE (at 0 0) (layer F.SilkS) hide
    (effects (font (size 1.27 1.27) (thickness 0.15)))
  )
  (pad GND@0 smd rect (at 0 0) (size 1.524 4.064) (layers F.Cu F.Mask)
    (solder_mask_margin 0.0508))
  (pad SIG smd rect (at 2.54 0) (size 1.524 4.064) (layers F.Cu F.Mask)
    (solder_mask_margin 0.0508))
  (pad GND@1 smd rect (at 5.08 0) (size 1.524 4.064) (layers F.Cu F.Mask)
    (solder_mask_margin 0.0508))
  (pad GND@2 smd rect (at 0 0) (size 1.524 4.064) (layers B.Cu B.Paste B.Mask)
    (solder_mask_margin 0.0508))
  (pad GND@3 smd rect (at 5.08 0) (size 1.524 4.064) (layers B.Cu B.Paste B.Mask)
    (solder_mask_margin 0.0508))
)
